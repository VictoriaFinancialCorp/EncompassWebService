<div class="container col-xs-offset-1 col-xs-10 col-sm-offset-2 col-sm-8 col-md-offset-3 col-md-6">
  <h2>Validate MIN</h2>

  <%- partial('../error.ejs') %>

  <form action="mers-validate" method="post" class="form-horizontal">



    <div class="form-group">
      <label class="col-sm-3 control-label">MIN Number</label>
      <div class="col-sm-9">
        <input type="text" class="form-control" name="min_num" value="<%= (typeof min_num == 'undefined') ? '' : min_num %>" required/>
      </div>
    </div>

    <button type="submit" class="btn btn-info pull-right">Validate</button>
  </form>
</div>
