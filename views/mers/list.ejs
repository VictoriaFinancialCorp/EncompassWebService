
<div class="container">
  <h1>MERS MIN</h1>


  <table id="table" class="table table-hover table-condensed">
    <thead>
      <tr>
        <th>Org ID</th>
        <th>Loan Number</th>
        <th>MIN</th>
        <th>Borrower Name</th>
        <th>Processed By</th>
        <th>Created</th>

      </tr>
    </thead>

    <tbody>
      <% loans.forEach(function(loan) { %>
        <tr>
          <td><%= loan.org_id %></td>
          <td><%= loan.loan_num %></td>
          <td><%= loan.min_num %></td>
          <td><%= loan.b_name %></td>
          <td><%= loan.processed_by %></td>
          <td><%= moment(loan.createdAt).format('MM/DD/YY  HH:mm:ss') %></td>
        </tr>

      <% }); %>

    </tbody>


  </table>
</div>

<script>
  document.addEventListener('DOMContentLoaded', function(){
    $("#table").DataTable({
       "pageLength": 10
    });
  }, false);
  <%
  function formatDate(date){
    if(date == null) return " ";
    return moment(date).format('MM/DD/YY');
  }
  %>
</script>
